   "source": [
    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def payoff(x):\n",
    "    if x < 1:\n",
    "        return 0.0\n",
    "    else:\n",
    "        return 1 - 1.0/x"
   ]
  },
